{
	"name": "@honestjs/hono-middleware",
	"version": "0.1.0",
	"description": "Hono middleware adapter for HonestJS framework",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"honestjs",
		"middleware",
		"hono"
	],
	"author": "Orkhan Karimov <karimovok1@gmail.com> (https://github.com/kerimovok)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/honestjs/middleware.git",
		"directory": "packages/hono-middleware"
	},
	"dependencies": {},
	"devDependencies": {},
	"peerDependencies": {
		"honestjs": "^0.1.0",
		"hono": "^4.0.0"
	}
}
